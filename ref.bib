@article{huhnRelationshipsGenotypeEnvironment1993,
  title = {Relationships between Genotype x Environment Interactions and Rank Orders for a Set of Genotypes Tested in Different Environments},
  author = {H{\"u}hn, M. and Lotito, S. and Piepho, H. P.},
  year = {1993},
  month = sep,
  journal = {Theoretical and Applied Genetics},
  volume = {86},
