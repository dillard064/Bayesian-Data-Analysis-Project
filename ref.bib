  keywords = {Applied Statistics,Biostatistics,Factor analytic models,Linear mixed models,Loss of information,Mixed Methods,Multi-environment trial,Multivariate Analysis,One-stage analysis,Planting Field Trials,Survey Methodology,Weighted two-stage analysis},
