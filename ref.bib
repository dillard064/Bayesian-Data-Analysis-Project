@phdthesis{smithMultiplicativeMixedModels1999,
  title = {Multiplicative Mixed Models for the Analysis of Multi-Environment Trial Data},
  author = {Smith, Alison B.},
  year = {1999},
  month = dec,
  urldate = {2025-02-20},
  abstract = {This thesis is concerned with the analysis of data from series of plant variety trials,  also known as Multi-Environment Trials (MET). These involve the evaluation of varieties in designed experiments at a number of locations and possibly over several years.  There is usually interest in obtaining an overall measure of performance for each variety (across environments) and in quantifying and interpreting variety by environment  (V{\texttimes}E) interactions.  MET data routinely exhibit heterogeneity of variance and covariance at several levels.  Error variance often differs between trials and spatial variation within trials is common.  In addition genetic variance differs between trials and there is heterogeneity of genetic  covariance between pairs of trials. This thesis presents a mixed model approach to the  analysis of MET data which accommodates all of these sources of heterogeneity.  Chapter 1 presents an overview of the issues associated with the analysis of MET data  and includes a review of existing approaches. The new approach developed in this  thesis is introduced.  Chapter 2 presents important results concerning the estimation of linear mixed models.  Topics covered include estimation of fixed and random effects, estimation of variance  parameters using Residual Maximum Likelihood (REML) and the Average Information  algorithm, inference and computing strategies.  Chapter 3 presents the spatial mixed model approach to the analysis of individual field  trials (Gilmour et al. , 1997). This is illustrated with examples in Chapter 4.  In Chapter 5 the new mixed model approach to the analysis of MET data is presented.  The model accommodates error variance heterogeneity between trials and spatial vari vi  ation within trials (as per Cullis et al. , 1998) and models the variety effects in each  environment (that is, the sum of variety main effects and V{\texttimes}E interactions) using the  Factor Analytic model (Mardia et al. , 1988). This consists of a multiplicative model  of variety ``scores'' and environment ``loadings''. It allows a separate genetic variance  for each trial and a parsimonious and interpretable model for the genetic correlations  between trials. Issues discussed include non-uniqueness of loadings (constraints for estimation and subsequent rotation for interpretation), model estimation, formal tests of  goodness-of-fit (number of multiplicative terms) and interpretation (bi-plots, obtaining  estimates of overall variety performance). The technique is illustrated with an example  in Chapter 6.  Chapter 7 discusses the explicit inclusion of variety main effects with the Factor Analytic model then applied to the V{\texttimes}E interactions. Issues covered include transformation of the loadings to avoid mis-interpretation of variety main effects and the consequent need for correlations between variety main effects and variety scores from the  multiplicative part of model. The chapter concludes with an example.  Chapter 8 presents results of a simulation study to examine the convergence behaviour  of Factor Analytic models. Algorithms investigated are the Average Information algorithm for estimation of all variance parameters and the Expectation-Maximisation  (EM) algorithm for estimation of factor loadings. The results suggest the need for  fitting multiplicative models with reduced rank variance structures. An estimation  method for such models is given in the final section of the chapter.  Chapter 9 presents concluding remarks including reference to the applicability of the  methodology to problems other than the analysis of MET data.},
  langid = {english},
  file = {/Users/jennylinmacbookm1pro/Zotero/storage/AG2445B2/Multiplicative mixed models for the analysis of multi-environment trial data  Alison B. Smith  The.pdf}
}

@article{smithPlantBreedingSelection2018,
  title = {Plant Breeding Selection Tools Built on Factor Analytic Mixed Models for Multi-Environment Trial Data},
  author = {Smith, Alison B. and Cullis, Brian R.},
