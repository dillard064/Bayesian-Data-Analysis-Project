  author = {Burgue{\~n}o, Juan and Crossa, Jos{\'e} and Cotes, Jos{\'e} Miguel and Vicente, Felix San and Das, Biswanath},
  year = {2011},
  journal = {Crop Science},
  volume = {51},
  number = {3},
  pages = {944--954},
  issn = {1435-0653},
  doi = {10.2135/cropsci2010.07.0403},
  urldate = {2025-05-19},
  abstract = {Fixed linear models have been used for describing genotype {\texttimes} environment interaction (GE). Previous attempts have been made to assess the predictive ability of some linear mixed models when GE components are treated as random effects and modeled by the factor analytic (FA) model. This study compares the predictive ability of linear mixed models when the GE is modeled by the FA model with that of simple linear mixed models when the GE is not modeled. A cross-validation scheme is used that randomly deletes some genotypes from sites; the values for these genotypes are then predicted by the different models and correlated with their observed values to assess model accuracy. A total of six multienvironment trials (one potato [Solanum tuberosum L.] trial, three maize [Zea mays L.] trials, and two wheat [Triticum aestivum L.] trials) with GE of varying complexity were used in the evaluation. Results show that for data sets with complex GE, modeling GE using the FA model improved the predictability of the model up to 6\%. When GE is not complex, most models (with and without FA) gave high predictability, and models with FA did not seem to lose much predictive ability. Therefore, we concluded that modeling GE with the FA model is a good thing.},
  langid = {english},
  file = {/Users/jennylinmacbookm1pro/Zotero/storage/IC23ASEK/Burgueño et al. - 2011 - Prediction Assessment of Linear Mixed Models for Multienvironment Trials.pdf;/Users/jennylinmacbookm1pro/Zotero/storage/589ASW3R/cropsci2010.07.html}
}

