@article{vaneeuwijkMultiplicativeModelsInteraction1998,
  title = {Multiplicative {{Models}} for {{Interaction}} in {{Three-Way ANOVA}}, with {{Applications}} to {{Plant Breeding}}},
  author = {{van Eeuwijk}, Fred A. and Kroonenberg, Pieter M.},
  year = {1998},
  journal = {Biometrics},
